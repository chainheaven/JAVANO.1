public class TEST41 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10, b = 0;
		try {
			System.out.println(a / b);
		} catch (ArithmeticException ae) {
			System.out.println("oops");
		}

		int[] c = { 1, 2, 3, 4 };
		System.out.println(c[3]);
	}

}
